{
  "name": "instacar/psr7-utils",
  "type": "library",
  "description": "A reusable utilities for PSR-7",
  "keywords": [
    "psr-7", "utils"
  ],
  "license": "LGPL-3.0-only",
  "authors": [
    {"name": "Brandon Antonio Lorenzo", "email": "bantonio@gtglobal.com"}
  ],
  "require": {
    "php": ">=8.1",
    "psr/http-factory": "^1.0.1",
    "psr/http-message": "^1.0.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13",
    "nyholm/psr7": "^1.5",
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "symfony/property-access": "^6.2",
    "symfony/serializer": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "Instacar\\Psr7Utils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Instacar\\Psr7Utils\\Test\\": "tests/"
    }
  },
  "scripts": {
    "lint": "vendor/bin/php-cs-fixer fix -q -n --dry-run",
    "analyze": "vendor/bin/phpstan analyse -q -n --no-ansi",
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
